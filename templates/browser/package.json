{
  "name": "<%= projectId %>",
  "version": "0.0.1",
  "description": "<%= projectName %> project",
  "main": "src/main.js",
  "scripts": {
    <% if (hasFlow) { %>"flow": "$(npm bin)/flow; test $? -eq 0 -o $? -eq 2",
    <% } %>"mocha-test": "$(npm bin)/mocha ./test/test.js",
    "mocha-html-test": "npm run mocha-test -- --reporter mochawesome --reporter-options reportDir=\"test/_report\",reportName=\"report\",reportTitle=\"Bedrock utils report\",inlineAssets=true",
    "eslint": "$(npm bin)/eslint",
    "eslint-test": "npm run eslint -- ./src/**/*.js ./styleguide/**/*.js ./sdk/**/*.js",
    "stylelint": "$(npm bin)/stylelint",
    "stylelint-test": "npm run stylelint -- ./src/**/*.scss ./styleguide/**/*.scss",
    <% if (hasFlow) { %>"test": "npm run eslint-test && npm run stylelint-test && npm run flow && npm run mocha-test",
    <% } else { %>"test": "npm run eslint-test && npm run stylelint-test && npm run mocha-test",
    <% } %>"build-styleguide": "$(npm bin)/gulp --gulpfile=\"$(npm bin)/bedrock-tasks-gulp\" styleguide --config=\".build.json\"",
    "base-build": "$(npm bin)/gulp --gulpfile=\"$(npm bin)/bedrock-tasks-gulp\" build --config=\".build.json\"",
    "build": "npm run base-build -- --env=dev",
    "build-prod": "npm run test && npm run base-build -- --env=prod",
    "build-analyze": "$(npm bin)/source-map-explorer ./public/theme/jnco/app.js",
    "build-analyze-json": "npm run build-analyze -- --json",
    "build-analyze-tsv": "npm run build-analyze -- --tsv",
    "build-analyze-html": "npm run build-analyze -- --html > ./logs/bundle-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://<TODO_SET_GIT>"
  },
  "author": "<TODO_SET_AUTHOR>",
  "license": "ISC",
  "homepage": "<TODO_SET_HOMEPAGE>",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.8",
    <% if (hasFlow) { %>"babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-comments": "^6.17.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    <% } %><% if (hasES6) { %>"babel-preset-es2015": "^6.18.0",
    <% } %>"bedrock-tasks": "git://github.com/Sendoushi/bedrock-tasks.git#0.0.1",
    "eslint": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^6.2.0",
    <% if (hasFlow) { %>"flow-bin": "^0.36.0",
    <% } %>"html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "postcss": "^5.2.0",
    "source-map-explorer": "^1.3.3",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^13.0.0",
    "stylelint-scss": "^1.3.4",
    "glob": "^7.1.1",
    "mocha": "^3.2.0",
    "mochawesome": "^1.5.4",
    "chai": "^3.5.0"
  },
  "dependencies": {
    "bedrock": "git://github.com/Sendoushi/bedrock.git#0.2.1",
    "bedrock-components": "git://github.com/Sendoushi/bedrock-components.git#0.0.1",
    "core-js": "^2.4.1"
  }
}
